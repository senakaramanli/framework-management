{
  "name": "compliance-frameworks-ui",
  "version": "1.0.0",
  "description": "Pixel-perfect compliance frameworks UI with Bootstrap 5, SASS, and Vanilla JS",
  "main": "public/index.html",
  "scripts": {
    "dev": "sass --no-source-map --watch public/main.scss public/main.css",
    "build:css": "sass --style=compressed public/main.scss public/main.css",
    "serve": "lite-server --config bs-config.json",
    "start": "concurrently \"npm run dev\" \"npm run serve\""
  },
  "devDependencies": {
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "concurrently": "^8.2.2",
    "lite-server": "^2.6.1",
    "sass": "^1.69.5"
  },
  "keywords": ["bootstrap", "sass", "vanilla-js", "compliance", "frameworks"],
  "author": "",
  "license": "MIT"
}
